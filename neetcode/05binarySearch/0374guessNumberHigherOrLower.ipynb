    "# my solution\n",
    "\n",
    "def guessNumber(self, n: int) -> int:\n",
    "    l = 1\n",
    "    r = n\n",
    "    \n",
    "    while l <= r:\n",
    "        mid = (l + r) // 2\n",
    "        if guess(mid) == -1:\n",
    "            r = mid - 1\n",
    "        elif guess(mid) == 1:\n",
    "            l = mid + 1\n",
    "        else:\n",
    "            return mid"
   ]
